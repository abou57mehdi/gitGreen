{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" }
      },
      {
        "src": "green_data_center/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "green_data_center/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "green_data_center.settings",
      "PYTHONUNBUFFERED": "1",
      "PYTHONPATH": "."
    }
  }
  